{
  "name": "portofino-upstairs",
  "version": "5.3.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-lib-dev": "ng build portofino --watch",
    "build-lib": "ng build --configuration production portofino",
    "document-lib": "compodoc -p projects/portofino/tsconfig.lib.json -d dist/documentation",
    "package-lib": "npm run build-lib && npm run npm-pack",
    "publish-lib": "npm run build-lib && cd dist/portofino && npm publish",
    "build-all": "npm run build-lib && ng build --configuration production",
    "npm-pack": "cd dist/portofino && npm pack",
    "maven-build": "ng build portofino && ng build --configuration production --delete-output-path && npm run npm-pack",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect -d"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "v12-lts",
    "@angular/cdk": "v12-lts",
    "@angular/common": "v12-lts",
    "@angular/compiler": "v12-lts",
