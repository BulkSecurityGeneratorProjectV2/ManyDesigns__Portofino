    "@angular/elements": "v12-lts",
    "@angular/flex-layout": "^12.0.0-beta.35",
    "@angular/forms": "v12-lts",
    "@angular/material": "v12-lts",
    "@angular/material-moment-adapter": "v12-lts",
    "@angular/platform-browser": "v12-lts",
    "@angular/platform-browser-dynamic": "v12-lts",
    "@angular/router": "v12-lts",
    "@ngx-translate/core": "^13.0.0",
    "@ngxd/core": "^12.0.1",
    "core-js": "^3.21.1",
    "file-input-accessor": "^12.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": ">=4.17.21",
    "lodash.merge": ">=4.6.2",
    "material-icons": "^1.10.4",
    "moment": "^2.29.1",
    "ngx-quill": "^14.3.0",
    "quill": "^1.3.7",
    "roboto-fontface": "^0.10.0",
    "rxjs": "^6.6.6",
    "snyk": "^1.685.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
