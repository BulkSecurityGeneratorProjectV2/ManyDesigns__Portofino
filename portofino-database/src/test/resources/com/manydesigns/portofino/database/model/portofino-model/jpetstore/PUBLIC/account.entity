import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "ACCOUNT")
entity account {
	id {
		@Column(name = "USERID")
		userid
	}
	@Column(name = "EMAIL")
	email
	@Column(name = "FIRSTNAME")
	firstname
	@Column(name = "LASTNAME")
	lastname
	@Column(name = "STATUS")
	status
	@Column(name = "ADDR1")
	addr1
	@Column(name = "ADDR2")
	addr2
	@Column(name = "CITY")
	city
	@Column(name = "STATE")
	state
	@Column(name = "ZIP")
	zip
	@Column(name = "COUNTRY")
	country
	@Column(name = "PHONE")
	phone
}