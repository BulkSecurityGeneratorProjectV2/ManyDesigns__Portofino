import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "PROFILE")
entity profile {
	id {
		@Column(name = "USERID")
		userid
	}
	@Column(name = "LANGPREF")
	langpref
	@Column(name = "FAVCATEGORY")
	favcategory
	@Column(name = "MYLISTOPT")
	mylistopt: boolean
	@Column(name = "BANNEROPT")
	banneropt: boolean
}