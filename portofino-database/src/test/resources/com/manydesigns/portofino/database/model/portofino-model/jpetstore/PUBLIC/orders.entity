import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "ORDERS")
entity orders {
	id {
		@Column(name = "ORDERID")
		orderid: EBigInteger
	}
	@Column(name = "USERID")
	userid
	@Column(name = "ORDERDATE")
	orderdate: java.sql.Date
	@Column(name = "SHIPADDR1")
	shipaddr1
	@Column(name = "SHIPADDR2")
	shipaddr2
	@Column(name = "SHIPCITY")
	shipcity
	@Column(name = "SHIPSTATE")
	shipstate
	@Column(name = "SHIPZIP")
	shipzip
	@Column(name = "SHIPCOUNTRY")
	shipcountry
	@Column(name = "BILLADDR1")
	billaddr1
	@Column(name = "BILLADDR2")
	billaddr2
	@Column(name = "BILLCITY")
	billcity
	@Column(name = "BILLSTATE")
	billstate
	@Column(name = "BILLZIP")
	billzip
	@Column(name = "BILLCOUNTRY")
	billcountry
	@Column(name = "COURIER")
	courier
	@Column(name = "TOTALPRICE")
	totalprice: EBigDecimal
	@Column(name = "BILLTOFIRSTNAME")
	billtofirstname
	@Column(name = "BILLTOLASTNAME")
	billtolastname
	@Column(name = "SHIPTOFIRSTNAME")
	shiptofirstname
	@Column(name = "SHIPTOLASTNAME")
	shiptolastname
	@Column(name = "CREDITCARD")
	creditcard
	@Column(name = "EXPRDATE")
	exprdate
	@Column(name = "CARDTYPE")
	cardtype
	@Column(name = "LOCALE")
	locale
}