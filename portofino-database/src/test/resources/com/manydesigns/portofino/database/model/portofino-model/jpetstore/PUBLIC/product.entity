import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "PRODUCT")
entity product {
	id {
		@Column(name = "PRODUCTID")
		productid
	}
	@Column(name = "CATEGORY")
	category
	@Column(name = "NAME")
	name
	@Column(name = "DESCN")
	descn
	FK_PRODUCT_1 --> category(catid=category)
}