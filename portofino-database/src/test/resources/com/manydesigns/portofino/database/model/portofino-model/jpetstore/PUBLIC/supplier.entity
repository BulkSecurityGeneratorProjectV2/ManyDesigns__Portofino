import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "SUPPLIER")
entity supplier {
	id {
		@Column(name = "SUPPID")
		suppid: EBigInteger
	}
	@Column(name = "NAME")
	name
	@Column(name = "STATUS")
	status
	@Column(name = "ADDR1")
	addr1
	@Column(name = "ADDR2")
	addr2
	@Column(name = "CITY")
	city
	@Column(name = "STATE")
	state
	@Column(name = "ZIP")
	zip
	@Column(name = "PHONE")
	phone
}