import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "ITEM")
entity item {
	id {
		@Column(name = "ITEMID")
		itemid
	}
	@Column(name = "PRODUCTID")
	productid
	@Column(name = "LISTPRICE")
	listprice: EBigDecimal
	@Column(name = "UNITCOST")
	unitcost: EBigDecimal
	@Column(name = "SUPPLIER")
	supplier: EBigInteger
	@Column(name = "STATUS")
	status
	@Column(name = "ATTR1")
	attr1
	@Column(name = "ATTR2")
	attr2
	@Column(name = "ATTR3")
	attr3
	@Column(name = "ATTR4")
	attr4
	@Column(name = "ATTR5")
	attr5
	FK_ITEM_2 --> supplier(suppid=supplier)
	FK_ITEM_1 --> product(productid=productid)
}