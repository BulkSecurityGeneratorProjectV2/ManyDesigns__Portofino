import javax.persistence.Column;
import javax.persistence.Table;

@Table(name = "ORDERSTATUS")
entity orderstatus {
	id {
		@Column(name = "ORDERID")
		orderid: EBigInteger
		@Column(name = "LINENUM")
		linenum: EBigInteger
	}
	@Column(name = "TIMESTAMP")
	timestamp: java.sql.Date
	@Column(name = "STATUS")
	status: string
}